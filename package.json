{
  "name": "band-manager-v2-node-server",
  "version": "2.0.0",
  "description": "node server for chat and interacting with google place api",
  "main": "app.js",
  "author": "Wesley Coderre",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon app.js",
    "db:create": "createdb -e band_manager_v2",
    "db:drop": "dropdb --if-exist -e band_manager_v2",
    "db:migrate": "knex migrate:latest",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate",
    "redis": "redis-server /usr/local/etc/redis.conf"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "connect-redis": "^4.0.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "joi": "^14.3.1",
    "knex": "^2.4.0",
    "morgan": "^1.9.1",
    "pg": "^7.12.1"
  },
  "devDependencies": {
    "faker": "^4.1.0",
    "nodemon": "^1.19.4"
  }
}
